  function MEESCalc() {
    var gcs = (document.getElementById("GCS"));
    var hr = (document.getElementById("HR"));
    var rr = (document.getElementById("RR"));
    var cr = (document.getElementById("CR"));
    var pain = (document.getElementById("Pain"));
    var bp = (document.getElementById("BP"));
    var os = (document.getElementById("OS"));
  

    var scoreDisp= document.getElementById("MEESDisp");
    var score = parseInt(document.getElementById("MEESScore"));

    var dd1 = parseInt(gcs.options[gcs.selectedIndex].value);
      var dd2 = parseInt(hr.options[hr.selectedIndex].value);
      var dd3 = parseInt(rr.options[rr.selectedIndex].value);
      var dd4 = parseInt(cr.options[cr.selectedIndex].value);
      var dd5 = parseInt(pain.options[pain.selectedIndex].value);
      var dd6 = parseInt(bp.options[bp.selectedIndex].value);
      var dd7 = parseInt(os.options[os.selectedIndex].value);
    
    scoreDisp.style.display = "block";

    var score = dd1 + dd2 + dd3 + dd4 + dd5 + dd6 + dd7;
    
    if(dd1 == 1 || dd2 == 1 || dd3 == 1 || dd4 == 1 || dd5 == 1 || dd6 == 1 || dd7 == 1)
    {
      document.getElementById("MEESScore").value = score + "*";
      document.getElementById("asterisk").style.display = "block";
    }
    else
    {
      document.getElementById("MEESScore").value = score;
    }

  }



